<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/docket_layout"
    android:visibility="visible"
    android:layout_marginEnd="@dimen/_16sdp"
    android:layout_marginStart="@dimen/_16sdp"
    android:layout_marginTop="@dimen/_12sdp"
    android:layout_height="wrap_content"
    tools:context="com.ritco.operationmanagement.activitys.ThcGenerat">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Docket No."
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:id="@+id/docket_no"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/secondry_text"
                    android:hint="Type Here.."
                    android:textColorHint="@color/secondry_text"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Docket Dt."
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edittext_out_fill_gray"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:id="@+id/docket_date"
                    android:paddingRight="@dimen/_10sdp"
                    android:drawablePadding="@dimen/_4sdp"
                    android:gravity="center_vertical"
                    android:drawableEnd="@drawable/form_calendar_icon"
                    android:textColor="@color/blue"
                    android:hint="DD/MM/YYYY"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Address"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/address"
                    android:background="@drawable/edittext_out_fill_gray"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/secondry_text"
                    android:hint="Type Here.."
                    android:textColorHint="@color/secondry_text"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_weight="1">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_medium"
                android:gravity="center_vertical"
                android:text="GST No."
                android:textColor="#454545"
                android:textSize="@dimen/_12ssp" />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_4sdp"
                android:background="@drawable/edittext_out_fill_gray"
                android:fontFamily="@font/roboto_regular"
                android:paddingLeft="@dimen/_10sdp"
                android:id="@+id/origin"
                android:textColor="@color/secondry_text"
                android:hint="GST No."
                android:textColorHint="@color/blue"
                android:textSize="@dimen/_12sdp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="From City"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/from_city"
                    android:gravity="center_vertical"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingStart="@dimen/_10sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:drawableEnd="@drawable/spiner_down_erro"
                    android:textColor="@color/blue"
                    android:text="Smart Search"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="From State"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/from_state"
                    android:background="@drawable/edittext_out_fill_gray"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:gravity="center_vertical"
                    android:textColor="@color/blue"
                    android:text="Rajasthan"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="To City"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/toCity"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingStart="@dimen/_10sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:drawableEnd="@drawable/spiner_down_erro"
                    android:textColor="@color/blue"
                    android:text="Smart Search"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="To State"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/to_state"
                    android:background="@drawable/edittext_out_fill_gray"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:gravity="center_vertical"
                    android:text="Rajasthan"
                    android:textColor="@color/blue"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Load Type"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_weight=".28"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:orientation="horizontal">

                    <Spinner
                        android:id="@+id/spinner_load_type"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:autofillHints="Consignor"
                        android:layout_marginLeft="5dp"
                        android:spinnerMode="dropdown" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:src="@drawable/spiner_down_erro" />

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Slab Type"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_weight=".28"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:orientation="horizontal">

                    <Spinner
                        android:id="@+id/spinner_slab_type"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:autofillHints="Consignor"
                        android:layout_marginLeft="5dp"
                        android:spinnerMode="dropdown" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:src="@drawable/spiner_down_erro" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Route Mode"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_weight=".28"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:orientation="horizontal">

                    <Spinner
                        android:id="@+id/spinner_route_mode"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:autofillHints="Consignor"
                        android:layout_marginLeft="5dp"
                        android:spinnerMode="dropdown" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:src="@drawable/spiner_down_erro" />

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_horizontal"
                    android:text="Transhipment"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:orientation="horizontal">
                 <RadioGroup
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:id="@+id/transhipment_grop"
                     android:layout_marginStart="@dimen/_16sdp"
                     android:orientation="horizontal">
                     <RadioButton
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="No"
                         android:textIsSelectable="true"
                         android:checked="true"
                         android:buttonTint="@color/blue"
                         android:id="@+id/transhipment_no"/>
                     <RadioButton
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Yes"
                         android:layout_marginStart="@dimen/_16sdp"
                         android:buttonTint="@color/blue"
                         android:id="@+id/transhipment_yes"/>

                 </RadioGroup>
             </LinearLayout>
            </LinearLayout>
        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:lines="1"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:text="Transhipment Location"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:id="@+id/transhipment_location"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/secondry_text"
                    android:hint="Type Here.."
                    android:textColorHint="@color/secondry_text"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Transhipment City"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/transhipment_city"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/secondry_text"
                    android:hint="Type Here.."
                    android:textColorHint="@color/secondry_text"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Tentative Freight/Rate"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:id="@+id/tentative_rate"
                    android:background="@drawable/edit_form_outline"
                    android:fontFamily="@font/roboto_regular"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textColor="@color/secondry_text"
                    android:hint="Type Here.."
                    android:textColorHint="@color/secondry_text"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_weight="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center_vertical"
                    android:text="Permitted By"
                    android:textColor="#454545"
                    android:textSize="@dimen/_12ssp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginLeft="3dp"
                    android:layout_weight=".28"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:background="@drawable/edit_form_outline"
                    android:orientation="horizontal">

                    <Spinner
                        android:id="@+id/spinner_permitted_by"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:autofillHints="Consignor"
                        android:layout_marginLeft="5dp"
                        android:spinnerMode="dropdown" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:src="@drawable/spiner_down_erro" />

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="vertical"
            android:layout_weight="1">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_medium"
                android:gravity="center_vertical"
                android:text="Transhipment Remarks"
                android:textColor="#454545"
                android:textSize="@dimen/_12ssp" />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginTop="@dimen/_4sdp"
                android:id="@+id/transhipment_remarks"
                android:background="@drawable/edit_form_outline"
                android:fontFamily="@font/roboto_regular"
                android:paddingLeft="@dimen/_10sdp"
                android:textColor="@color/secondry_text"
                android:gravity="top"
               android:padding="@dimen/_4sdp"
                android:textColorHint="@color/secondry_text"
                android:textSize="@dimen/_12sdp" />
        </LinearLayout>





        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/_18sdp"
            android:layout_marginBottom="@dimen/_16sdp"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/bil_next"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_30sdp"
                android:layout_alignParentRight="true"
                android:background="@drawable/blue_button"
                android:fontFamily="@font/poppins_medium"
                android:gravity="center"
                android:paddingEnd="@dimen/_10sdp"
                android:paddingStart="@dimen/_10sdp"
                android:drawablePadding="@dimen/_8sdp"
                android:drawableEnd="@drawable/form_but_next"
                android:drawableTint="@color/white"
                android:onClick="bill_next"
                android:text="Next"
                android:textColor="@color/white"
                android:textSize="@dimen/_12ssp" />
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
